#include<iostream>
#include<algorithm>
#include<vector>

using namespace std;

int N;
int val[1001];
int dp[1001];
int result = 0;
bool Check[1001][1001];
int Find;
int main(void) {
	cin >> N;
	for (int i = 0; i < N; i++) {
		cin >> val[i];
	}
	for (int i = 0; i < N; i++) {
		dp[i] = 1;
		Check[i][i] = 1;
		for (int j = 0; j < i; j++) {

			if (val[i] > val[j]) {
				if (dp[i] < dp[j] + 1) {
					dp[i] = dp[j] + 1;

					for (int k = 0; k < N; k++) {
						Check[i][k] = Check[j][k];
					}
							Check[i][i] = 1;
				}
			}
		}
		if (dp[i] > result) {
			result = dp[i];
			Find = i;
		}
	}

	cout << result<<endl;
	for (int i = 0; i < N; i++) {
		if (Check[Find][i]) { cout << val[i] << " "; }
	}
}
